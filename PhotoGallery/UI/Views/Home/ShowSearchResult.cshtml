@model UI.Models.ImageReview
@{
    Layout = null;
}
<p>
    <a class="position-right" href="#" onclick="closeForm('#workForm', 'search-form');">x</a>
</p>
@if (Model != null)
{      
    <p>
        @Resources.Resources.SearchResultsByRequest @ViewBag.Name
    </p>
    foreach (var image in Model.Images)
    {
    <a class="th image-search-result-size" href="#" onclick="ShowImage(@image.ImageId); closeForm('#workForm', 'search-form'); setImageIndexByImageId(@image.ImageId);">
        <img src="@image.ImagePicture" />
    </a> 
    }
}
else
{
    <p>
        @ViewBag.Name
    </p>    
}